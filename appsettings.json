{
  "Middleware": {
    "ApiKeyAuthentication:ApiKeyToIdentityName": {
    },
    "JwtAuthentication": {
      "Issuer": "",
      "Audience": "",
      "Algorithms": [],
      "IssuerSigningKey": "",
      "ClockSkewSec": 90
    },
    "RequestTimeouts:TimeoutMs": 3000,
    "RateLimiting": {
      "ConcurrencyPermitLimit": 1000,
      "ConcurrencyQueueLimit": 100,
      "IpPermitLimit": 450,
      "IpWindowSec": 60
    },
    "TraceGuid:ReadFromRequestIfPresent": true,
    "IdempotentPosts": {
      "KeyExpirationHours": 10,
      "UserKeyMinLength": 12,
      "UserKeyMaxLength": 64
    },
    "IdempotentPostsInMemoryCache": {
      "CacheSec": 60,
      "KeyLimit": 2048,
      "ExpirationScanFrequencySec": 90
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext"
    ],
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "File",
              "Args": {
                "path": "Logs/On-.json",
                "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact",
                "rollingInterval": "Day",
                "buffered": true
              }
            },
            {
              "Name": "Console",
              "Args": {
                // "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{Level}] {SourceContext} {Message:l} {Properties:l}{NewLine}{Exception}",
                "formatter": "Serilog.Formatting.Compact.RenderedCompactJsonFormatter, Serilog.Formatting.Compact"
              }
            }
          ]
        }
      }
    ],
    "Properties": {
      "Application": "SawyerWebApiCtlrs"
    }
  },
  "AllowedHosts": "*"
}
